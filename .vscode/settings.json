  "files.associations": {
    "**/.azure-pipelines.old/*.yml": "azure-pipelines",
    "algorithm": "c",
    "sstream": "cpp",
    "assert.h": "c",
    "meta.h": "c",
    "config.h": "c",
    "dirent.h": "c",
    "crawler.h": "c",
    "file_info.h": "c"
  },
  "debug.allowBreakpointsEverywhere": true,
  "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools",
  "editor.fontFamily": "monospace",
  "editor.fontLigatures": true
}

