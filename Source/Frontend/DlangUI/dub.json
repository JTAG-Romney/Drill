{
	"authors": [
		"Federico Santamorena"
	],
	"buildTypes": {
		"debug": {
			"buildOptions": [
				"debugMode",
				"debugInfo",
				"alwaysStackFrame",
				"stackStomping"
			]
		},
		"release": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"noBoundsCheck",
				"optimize"
			]
		},
		"release-debug": {
			"buildOptions": [
				"debugInfo",
				"alwaysStackFrame",
				"stackStomping",
				"inline",
				"optimize"
			]
		}
	},
	"copyFiles": [
		"../../../Assets"
	],
	"copyright": "Copyright Â© 2019, Federico Santamorena",
	"dependencies": {
		"dlangui": "~>0.9.182",
		"drill-core": {
			"path": "../../Backend",
			"version": ">=0.0.0"
		}
	},
	"description": "Search files without indexing, but clever crawling.",
	"dflags": [
		"-J=\"../../../Assets\""
	],
	"homepage": "https://github.com/yatima1460/Drill",
	"importPaths": [
		"../../Backend"
	],
	"libs-posix": [
		"gtk-3",
		"gobject-2.0",
		"glib-2.0",
		"gio-2.0"
	],
	"license": "GPL-2.0",
	"mainSourceFile": "Main.d",
	"name": "drill-dlangui",
	"sourceFiles": [
		"Main.d"
	],
	"targetName": "drill-search-dlangui",
	"targetPath": "Build/Drill-dlangui-$PLATFORM-$ARCH-$BUILD_TYPE",
	"targetType": "executable",
	"toolchainRequirements": {
		"dmd": ">=2.86.0",
		
		"ldc": ">=1.15.0"
	}
